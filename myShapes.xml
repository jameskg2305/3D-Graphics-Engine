	<shape>
		<id>pyramid</id>
		
		<coord x = "0" y = "0" z = "100"/>
		
		<faceNumbers n = "4"/>
		
		<edgesForFaces>
			<edge>3</edge>
			 <edge>3</edge>
			<edge>3</edge>
			<edge>3</edge> 
		</edgesForFaces>
		
		<cornersForFaces>
			<cornersForFace>
				
				<corner x = "-30" y = "30" z = "-30" />
				<corner x = "30" y = "30" z = "-30" />
				<corner x = "0" y = "0" z = "0" />
			</cornersForFace>
			 <cornersForFace>
				<corner x = "-30" y = "30" z = "30" />
				
				<corner x = "-30" y = "30" z = "-30" />
				
				<corner x = "0" y = "0" z = "0" />
				
			</cornersForFace>
			<cornersForFace>
				<corner x = "-30" y = "30" z = "30" />
				
				<corner x = "30" y = "30" z = "30" />
				
				<corner x = "0" y = "0" z = "0" />
				
			</cornersForFace>
			<cornersForFace>
				<corner x = "30" y = "30" z = "30" />
				
				<corner x = "30" y = "30" z = "-30" />
				
				<corner x = "0" y = "0" z = "0" />
				
			</cornersForFace> 
		</cornersForFaces>
		
		<boundries>
			<face>
				<boundary x = "-30" y = "0" z = "0"/>
				<boundary x = "30" y = "0" z = "0"/>
				<boundary x = "-30" y = "30" z = "-30"/>
				<boundary x = "30" y = "30" z = "-30"/>
			</face>
			 <face>
				<boundary x = "0" y = "0" z = "-30"/>
				<boundary x = "0" y = "0" z = "30"/>
				<boundary x = "-30" y = "30" z = "-30"/>
				<boundary x = "-30" y = "30" z = "30"/>
			</face>
			<face>
				<boundary x = "30"   y = "0"  z = "0"/>
				<boundary x = "-30"   y = "0"  z = "0"/>
				<boundary x = "30" y = "30"  z = "30"/>
				<boundary x = "-30"  y = "30"  z = "30"/>
			</face>
			<face>
				<boundary x = "0" y = "0" z = "-30"/>
				<boundary x = "0" y = "0" z = "30"/>
				<boundary x = "30" y = "30" z = "-30"/>
				<boundary x = "30" y = "30" z = "30"/>
			</face> 
		</boundries>
		
		<images>
			<image>logo3.png</image>
			 <image>logo2.png</image>
			<image>logo1.png</image>
			<image>logo4.png</image> 
		</images>
		
	</shape>
